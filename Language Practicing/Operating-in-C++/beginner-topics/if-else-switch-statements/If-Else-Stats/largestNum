
#include <iostream>
using namespace std;

int main() {

    int x = 15, y = 20, z = 10;
    int largest;
    
    if (x > y) {
        largest = x;
    } else if (y > z) {
        largest = y;
    } else if (z > largest) {
        largest = z;
    } 

    cout << "The largest number is: " << largest << endl;

    return 0;
}
